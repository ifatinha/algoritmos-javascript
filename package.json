{
  "name": "algoritmos-javascript",
  "version": "1.0.0",
  "description": "Coleção de algoritmos e exercícios em JavaScript para estudo e prática.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "docs": "jsdoc -c jsdoc.json && npm run gerar-docs-index",
    "prepare": "husky",
    "gerar-docs-index": "node scripts/gerarIndex.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifatinha/algoritmos-javascript.git"
  },
  "keywords": [
    "javascript",
    "algoritmos",
    "exercicios",
    "estudo"
  ],
  "author": "Fatima Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifatinha/algoritmos-javascript/issues"
  },
  "homepage": "https://github.com/ifatinha/algoritmos-javascript#readme",
  "dependencies": {
    "jtest": "^0.0.1",
    "readline-sync": "^1.4.10",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.0",
    "babel-jest": "^30.2.0",
    "clean-jsdoc-theme": "^4.3.0",
    "commitlint": "^20.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jsdoc": "^4.0.5",
    "jsdoc-babel": "^0.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
